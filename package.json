{
  "name": "@serverless-devs/core",
  "version": "0.1.22",
  "description": "Serverless Devs Tool Core Component",
  "keywords": [
    "Serverless",
    "Serverless-Devs",
    "Serverless-Core"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Serverless-Devs/core"
  },
  "bugs": {
    "url": "https://github.com/Serverless-Devs/core/issues"
  },
  "homepage": "https://www.serverless-devs.com",
  "scripts": {
    "prewatch": "rimraf lib && mkdir lib && cp -r ./src/daemon ./lib",
    "watch": "npx tsc -w -p tsconfig.json",
    "debug": "ts-node test/fixtures/spinner.ts",
    "test": "npx jest",
    "prebuild": "rimraf lib && rimraf dist",
    "build": "npx tsc && cp -r ./src/daemon ./lib",
    "postbuild": "npm run esbuild && cp -r ./src/daemon ./dist",
    "esbuild": "esbuild lib/index.js --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --external:electron --outfile=dist/index.js",
    "test:cov": "jest --coverage",
    "lint": "npm run typecheck && f2elint scan -i src",
    "fix": "f2elint fix",
    "pub": "npm run build && npm publish",
    "typecheck": "npx tsc -p tsconfig.json --noEmit",
    "beta": "npm publish --tag=beta"
  },
  "autoInstall": false,
  "main": "./dist/index.js",
  "devDependencies": {
    "@serverless-devs/s-progress-bar": "^0.0.8",
    "@types/archiver": "^5.1.1",
    "@types/command-line-usage": "^5.0.1",
    "@types/download": "^6.2.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/rimraf": "^3.0.0",
    "ansi-escapes": "^4.3.2",
    "archiver": "^5.3.0",
    "chalk": "^4.1.0",
    "command-line-usage": "^6.1.1",
    "crypto-js": "^4.0.0",
    "decompress": "^4.2.1",
    "download": "^8.0.0",
    "esbuild": "^0.13.8",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "getmac": "^5.20.0",
    "got": "^8.3.2",
    "husky": "^6.0.0",
    "ignore": "^5.1.8",
    "inquirer": "^8.2.0",
    "ip": "^1.1.5",
    "is-docker": "2.2.1",
    "js-yaml": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "minimist": "^1.2.5",
    "ora": "^5.3.0",
    "prettyoutput": "^1.2.0",
    "readline": "^1.3.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "typescript": "^4.1.3",
    "uid": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "f2elint exec commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "f2elint exec lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "gitHead": "aec19d7a89cb711f0a246ecf77e79d3668fcb8da"
}
