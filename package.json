{
  "name": "@serverless-devs/core",
  "version": "0.0.99",
  "description": "Serverless Devs Tool Core Component",
  "keywords": [
    "Serverless",
    "Serverless-Devs",
    "Serverless-Core"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Serverless-Devs/core"
  },
  "bugs": {
    "url": "https://github.com/Serverless-Devs/core/issues"
  },
  "homepage": "https://www.serverless-devs.com",
  "scripts": {
    "watch": "tsc -w",
    "dev": "npx tsc -w -p tsconfig.json",
    "ncc": "ncc build lib/index.js -o dist",
    "debug": "ts-node test/fixtures/spinner.ts",
    "test": "npx jest",
    "build": "npx tsc",
    "test:cov": "jest --coverage",
    "lint": "npm run typecheck && f2elint scan -i src",
    "fix": "f2elint fix",
    "pub": "npm run build && npm publish",
    "typecheck": "npx tsc -p tsconfig.json --noEmit"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "@serverless-devs/s-progress-bar": "^0.0.8",
    "@tsed/logger": "^5.5.5",
    "archiver": "^5.2.0",
    "chalk": "^4.1.0",
    "command-line-usage": "^6.1.1",
    "crypto-js": "^4.0.0",
    "decompress": "^4.2.1",
    "download": "^8.0.0",
    "fs-extra": "9.0.1",
    "got": "^8.3.2",
    "i18n": "^0.13.2",
    "ignore": "^5.1.8",
    "inquirer": "^7.3.3",
    "js-yaml": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "minimist": "^1.2.5",
    "ora": "^5.3.0",
    "readline": "^1.3.0",
    "rimraf": "^3.0.2",
    "uid": "^2.0.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.1.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/download": "^6.2.4",
    "@types/fs-extra": "9.0.1",
    "@types/inquirer": "^7.3.1",
    "@types/rimraf": "^3.0.0",
    "@vercel/ncc": "^0.28.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "f2elint exec commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "f2elint exec lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "gitHead": "aec19d7a89cb711f0a246ecf77e79d3668fcb8da"
}
